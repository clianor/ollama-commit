  "license": "MIT",
  "homepage": "https://github.com/clianor/ollama-commit",
  "dependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.4",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "ollama": "^0.3.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
