{
  "name": "ollama-commit",
  "version": "0.0.10",
  "type": "module",
  "description": "Commit message generator with Ollama",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "ollama-commit": "dist/index.cjs"
  },
  "files": [
    "dist/**"
  ],
